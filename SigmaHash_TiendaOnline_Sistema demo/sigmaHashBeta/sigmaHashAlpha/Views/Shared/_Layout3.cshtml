<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewData["Title"] - sigmaHashAlpha</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/imports.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/sigmaHashAlpha.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css"/>
        <link rel="stylesheet" href="~/css/Manager.css" asp-append-version="true" />
     <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
</head>
<body>
    <div class="bodycontainer">
        <header>
            <nav class="navbar navbar-expand-sm navbar-toggleable-sm  navbaroptions ">
                <div class="container-fluid">
                    <a class="webtitle" asp-area="" asp-controller="Home" asp-action="Index">sigma.Hash</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">

                        <partial name="_LoginPartial" />
                    </div>

                    <div class="mgrfastbtns">
                        <i class="bi bi-gear"></i> <a asp-controller="Manager" asp-action="Index">Manager Area</a>
                    </div>
                </div>
            </nav>
        </header>
   
             

            <main role="main" class="mainbody3">

                

				<div class="GeneralContainer StockContainer" style="min-height: 95vh;">
				
				
					
					<div class="ManagerDiv" >
						
						<div class="ManagerMenu">
					
							<ul>
                                	<li><i class="bi bi-person-fill"></i></i><a  class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Usuario</a></li> 

									<li><a asp-controller="Products" asp-action="ListEditor"><i class="bi bi-card-list"></i>Mis Reservas</a></li>

									<li><a asp-controller="Products" asp-action="ListEditor"><i class="bi bi-list-check"></i>Mis Compras</a></li>



				
							</ul>
				
						</div>
						<div class="layout3body">
								 @RenderBody()
						</div>
						
				
					</div>
				
				
				</div>

               
            </main>


    </div>
        
       
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>

        @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
